{
  "name": "fm-data-api-client",
  "version": "1.1.1",
  "description": "FileMaker Data API Client",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "tslint -p tsconfig.json --fix",
    "lint": "eslint .",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:soliantconsulting/fm-data-api-client.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "FileMaker",
    "REST",
    "API",
    "Data",
    "Typescript"
  ],
  "author": "Soliant Consulting",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@js-joda/core": "^4.2.1",
    "@tsconfig/node14": "^1.0.1",
    "@types/form-data": "^2.5.0",
    "@types/into-stream": "^3.1.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@types/node-fetch": "^2.5.7",
    "eslint": "^8.13.0",
    "eslint-config-dasprid": "^0.1.12",
    "husky": "^7.0.4",
    "jest": "^26.4.2",
    "lint-staged": "^12.3.8",
    "node-fetch": "^2.6.0",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "into-stream": "^6.0.0"
  },
  "peerDependencies": {
    "@js-joda/core": "^3.0.0 || ^4.0.0",
    "node-fetch": "^2.6.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
